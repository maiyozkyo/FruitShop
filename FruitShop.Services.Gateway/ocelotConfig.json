        {
          "Host": "localhost",
          "Port": 8001
        }
      ],
      "UpstreamPathTemplate": "/User/LoginAsync",
      "UpstreamHttpMethod": [ "POST" ],
      "Priority": 2
    },
    {
      "DownstreamPathTemplate": "/api/User/RegisterAsync",
      "DownstreamScheme": "https",
